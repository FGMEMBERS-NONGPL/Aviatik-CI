<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- ALTIMETER -->
  <model>
    <path>Aircraft/Aviatik-CI/Models/instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.568 </x-m>
      <y-m>  0.2165 </y-m>
      <z-m>  0.494 </z-m>
    </offsets>
  </model>
  
  <!-- TURN -->
  <model>
    <path>Aircraft/Aviatik-CI/Models/instruments/turn/turn.xml</path>
    <offsets>
      <x-m> 0.567 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.483 </z-m>
    </offsets>
  </model>
  
  <!-- FUEL SELECTOR -->
  <model>
    <path>Aircraft/Aviatik-CI/Models/instruments/fuelselector/fuelselector.xml</path>
    <offsets>
      <x-m> 0.59 </x-m>
      <y-m> -0.070 </y-m>
      <z-m> 0.16 </z-m>
    </offsets>
  </model>
  
  <!-- PUMP SELECTOR -->
